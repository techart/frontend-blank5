{
	"name": "tao-webpack",
	"version": "5.14.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build-dev": "NODE_ENV=dev webpack --progress",
		"build-prod": "NODE_ENV=prod webpack --progress",
		"watch": "NODE_ENV=dev webpack --progress --watch",
		"start": "NODE_ENV=hot webpack serve",
		"pkg": "yarn",
		"with-aos": "yarn add aos@latest",
		"with-bem": "yarn add @webtechart/tao-bem@latest && node utils/with/bem.js",
		"with-gsap": "yarn add gsap@latest",
		"with-imwp": "yarn add -D image-minimizer-webpack-plugin sharp svgo",
		"with-jquery": "yarn add jquery@latest && node utils/with/jquery.js",
		"with-mobx": "yarn add -D mobx@latest",
		"with-react": "yarn add -D react@latest react-dom@latest react-hook-form@latest react-input-mask@latest prop-types@latest @svgr/webpack@latest eslint-plugin-react@latest mobx@latest mobx-react-lite@latest clsx@latest && node utils/with/react.js",
		"with-storybook": "yarn add -D @storybook/html@^6.5.9 @storybook/builder-webpack5@^6.5.9 @storybook/addon-actions@^6.5.9 @storybook/addon-essentials@^6.5.9 @storybook/addon-interactions@^6.5.9 @storybook/addon-links@^6.5.9 @storybook/manager-webpack5@^6.5.9 @storybook/testing-library@^0.0.13 html-loader@^4.2.0 && node utils/with/storybook.js",
		"with-swiper": "yarn add swiper@latest",
		"with-ts": "yarn add -D typescript@latest ts-loader@latest && node utils/with/ts.js",
		"with-vue": "yarn add -D vue@latest vue-loader@latest @vue/compiler-sfc@latest eslint-plugin-vue@latest && node utils/with/vue.js"
	},
	"author": "web@techart.ru",
	"license": "ISC",
	"dependencies": {
		"normalize.css": "^8.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"assets-webpack-plugin": "^7.1.1",
		"autoprefixer": "^10.4.4",
		"babel-loader": "^8.2.5",
		"browserslist": "latest",
		"caniuse-lite": "latest",
		"css-loader": "^5.2.7",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"directory-named-webpack-plugin": "^4.0.1",
		"eslint": "^8.1.0",
		"eslint-webpack-plugin": "^3.1.0",
		"expose-loader": "^3.1.0",
		"file-loader": "^6.2.0",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.3.3",
		"postcss-loader": "^6.2.1",
		"postcss-sort-media-queries": "^5.2.0",
		"sass": "^1.50.1",
		"sass-loader": "^12.6.0",
		"stylelint": "^14.7.1",
		"stylelint-config-standard-scss": "^5.0.0",
		"stylelint-webpack-plugin": "^3.2.0",
		"url-loader": "^4.1.1",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.15.1"
	}
}
